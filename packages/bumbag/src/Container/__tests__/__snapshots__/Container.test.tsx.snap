  max-width: 1440px;
  margin-left: auto;
  margin-right: auto;
}

@media (max-width: 768px) {
  .emotion-0 {
    padding-left: 1rem;
    padding-right: 1rem;
  }
}

.emotion-0 {
  background-color: var(--bb-palette-red, #ff0000);
}

@media (max-width: 1568px) {
  .emotion-0 {
    max-width: 1200px;
  }
}

@media (max-width: 1328px) {
  .emotion-0 {
    max-width: 1024px;
  }
}

@media (max-width: 1152px) {
  .emotion-0 {
    max-width: 768px;
  }
}

.emotion-1 {
  margin: unset;
  padding: unset;
  border: unset;
  background: unset;
  font: unset;
  font-family: inherit;
  font-size: 100%;
  box-sizing: border-box;
}

.emotion-1:focus:not(:focus-visible) {
  outline: none;
}

<div
  class="emotion-0 emotion-1 emotion-2 bb-Box bb-Container"
>
  hello world
</div>
`;

