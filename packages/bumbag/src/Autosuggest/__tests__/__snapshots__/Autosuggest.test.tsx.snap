  box-shadow: var(--bb-palette-primary200,#000000) 0px 0px 0px 3px;
}

.emotion-12 {
  position: relative;
}

.emotion-12 {
  background-color: var(--bb-palette-red,#ff0000);
}

<div
  aria-expanded="false"
  aria-haspopup="listbox"
  aria-owns="test"
  class="emotion-12 emotion-1 emotion-2 bb-Box bb-Autosuggest"
  role="combobox"
>
  <div
    class="emotion-1 emotion-2 emotion-5 emotion-6 bb-AutosuggestInput bb-InputWrapper bb-Box"
  >
    
    <input
      aria-autocomplete="list"
      aria-controls="test"
      aria-expanded="false"
      aria-haspopup="menu"
      aria-invalid="false"
      class="emotion-0 emotion-1 emotion-2 bb-Box bb-Input"
      loop=""
      placeholder="Search for a fruit..."
      role="textbox"
      type="text"
      value=""
    />
  </div>
  <div
    aria-orientation="vertical"
    class="emotion-7 emotion-8 emotion-1 emotion-10 emotion-11 bb-AutosuggestPopover bb-Box bb-DropdownMenuPopover"
    data-dialog="true"
    hidden=""
    id="test"
    loop=""
    role="listbox"
    style="display: none;"
  />
</div>
`;
